{
    "name": "faizulramir/senangpay",
    "description": "A Laravel package for integrating SenangPay payment gateway",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Faizulramir\\Senangpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faizulramir\\Senangpay\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faizulramir\\Senangpay\\SenangpayServiceProvider"
            ],
            "aliases": {
                "Senangpay": "Faizulramir\\Senangpay\\Facades\\Senangpay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "senangpay",
        "payment",
        "gateway",
        "malaysia"
    ],
    "homepage": "https://github.com/faizulramir/senangpay",
    "support": {
        "issues": "https://github.com/faizulramir/senangpay/issues",
        "source": "https://github.com/faizulramir/senangpay"
    }
}